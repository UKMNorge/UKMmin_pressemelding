{# FLASHBAG	#}
{% for type, messages in app.session.flashBag.all %}
	{% for message in messages %}
		{% if type == 'error' %}
			<div class="alert alert-danger">{{ message|trans }}</div>
		{% else %}	
			<div class="alert alert-{{ type }}">{{ message|trans }}</div>
		{% endif %}
	{% endfor %}
{% endfor %}

